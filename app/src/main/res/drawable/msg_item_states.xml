<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:telehealth="http://schemas.android.com/apk/res/org.tmr.telehealth">
    <item android:state_pressed="true"
        android:drawable="@android:color/transparent" /> <!-- pressed -->
    <item android:state_focused="true"
        android:drawable="@android:color/transparent" /> <!-- focused -->
    <item telehealth:state_message_unread="true"
        android:drawable="@android:color/white" />
    <item android:drawable="@android:color/white" /> <!-- default -->
</selector>